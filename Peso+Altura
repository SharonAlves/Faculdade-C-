#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main (){
  float altura, peso;
  printf("Digite su altura: ");
  scanf("%f", &altura);
  printf("Digite seu peso: ");
  scanf("%f",&peso);
  if ( altura<=1.7 && peso>+80) {
    	printf("Já era precisa emagrecer");
    }else{
        printf("UHU esta bem na fita");
 }
	return 0;
}
